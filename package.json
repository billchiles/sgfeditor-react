{
  "main": "electron/dist/main.js",
  "name": "sgfeditor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:web": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev:electron": "concurrently -k -r \"vite\" \"tsc -p electron/tsconfig.main.json -w\" \"tsc -p electron/tsconfig.preload.json -w\" \"wait-on http://localhost:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 ELECTRON_PRELOAD_PATH=preload.js electron .\"",
    "build:electron": "tsc -p electron/tsconfig.main.json && tsc -p electron/tsconfig.preload.json",
    "start:electron": "cross-env ELECTRON_PRELOAD_PATH=preload.js electron .",
    "app:prod": "npm run build && npm run build:electron && npm run start:electron"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^38.1.2",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "wait-on": "^9.0.1"
  }
}
